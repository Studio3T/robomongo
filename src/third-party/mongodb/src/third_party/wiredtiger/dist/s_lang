#! /bin/sh

# Check lang directories for potential name conflicts
t=__wt.$$
trap 'rm -f $t; exit 0' 0 1 2 3 13 15

cd ../lang

for d in *; do
	f=`find $d -name 'wiredtiger_wrap.c'`
	test -z "$f" && continue

	sed -e '/SWIGINTERN.*__wt_[a-z][a-z]*_[a-z]/!d' \
	    -e '/__wt_[^(]*__.*(/d' \
	    -e '/_wrap/d' \
	    -e "/_${d}_/d" \
	    $f > $t

	test -s $t && {
		echo "=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-="
		echo "$l: potential SWIG naming conflict"
		echo "=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-="
		cat $t
	}
done
