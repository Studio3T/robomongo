#!/bin/sh
set -e

if test -e /usr/bin/@PROJECT_NAME@ ; then
    rm /usr/bin/@PROJECT_NAME@
fi

if test -e /usr/share/applications/@PROJECT_NAME@.desktop ; then
    rm /usr/share/applications/@PROJECT_NAME@.desktop
fi

if test -e /usr/share/icons/@PROJECT_NAME@.png ; then
    rm /usr/share/icons/@PROJECT_NAME@.png
fi

